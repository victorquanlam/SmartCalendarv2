{
  "index": "/index.html",
  "dataGroups": [{
    "name": "api-freshness",
    "urls": [
      "/time-table",
      "/report",
      "/manage",
      "/dashboard",
      "/"
    ],
    "cacheConfig": {
      "maxSize": 10000,
      "maxAge": "1h",
      "timeout": "2s",
      "strategy": "freshness"
    }
  }, {
    "name": "api-performance",
    "urls": [
    ],
    "cacheConfig": {
      "maxSize": 10000,
      "maxAge": "1d",
      "strategy": "performance"
    }
  }],

  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
